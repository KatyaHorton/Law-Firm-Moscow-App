C \\u0438\\u0437-\\u0437\\u0430 \\u043D\\u0430\\u0447\\u0430\\u0432\\u0448\\u0435\\u0433\\u043E\\u0441\\u044F \\u0431\\u0440\\u0430\\u043A\\u043E\\u0440\\u0430\\u0437\\u0432\\u043E\\u0434\\u043D\\u043E\\u0433\\u043E \\u043F\\u0440\\u043E\\u0446\\u0435\\u0441\\u0441\\u0430. \"), React.createElement(\"p\", {\n        __source: {\n          fileName: _jsxFileName,\n          lineNumber: 30\n        },\n        __self: this\n      }, \"\\u0420\\u0435\\u0448\\u0438\\u0442\\u0435 \\u0432\\u0441\\u0435 \\u0441\\u0435\\u043C\\u0435\\u0439\\u043D\\u044B\\u0435  \\u0432\\u043E\\u043F\\u0440\\u043E\\u0441\\u044B \\u0431\\u0435\\u0437 \\u044D\\u043C\\u043E\\u0446\\u0438\\u0439 \\u043D\\u0430 \\u0432\\u044B\\u0441\\u043E\\u043A\\u043E\\u043C \\u043F\\u0440\\u043E\\u0444\\u0435\\u0441\\u0441\\u0438\\u043E\\u043D\\u0430\\u043B\\u044C\\u043D\\u043E\\u043C \\u0443\\u0440\\u043E\\u0432\\u043D\\u0435 \\u0441 \\u043D\\u0430\\u0448\\u0435\\u0439 \\u043F\\u043E\\u0434\\u0434\\u0435\\u0440\\u0436\\u043A\\u043E\\u0439!\"), React.createElement(\"p\", {\n        __source: {\n          fileName: _jsxFileName,\n          lineNumber: 32\n        },\n        __self: this\n      }, React.createElement(Link, {\n        to: \".\",\n        __source: {\n          fileName: _jsxFileName,\n          lineNumber: 32\n        },\n        __self: this\n      }, \"\\u0412\\u043E\\u0437\\u0432\\u0440\\u0430\\u0442 \\u043A \\u0441\\u043F\\u0438\\u0441\\u043A\\u0443\")));\n    }\n  }]);\n\n  return ServiceFamilyLaw;\n}(Component);\n\nexport default ServiceFamilyLaw;","map":{"version":3,"sources":["/Users/markhorton/Desktop/moscow-law-firm-app/client/src/components/main-slide-content/services/ServiceFamilyLaw.js"],"names":["React","Component","Link","ServiceFamilyLaw"],"mappings":";;;;;;AAAA,OAAOA,KAAP,IAAgBC,SAAhB,QAAiC,OAAjC;AACA,SAASC,IAAT,QAAqB,kBAArB;;IAEMC,gB;;;;;;;;;;;;;6BAEK;AACP,aACI;AAAK,QAAA,SAAS,EAAC,gBAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SACL;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,QADK,EAEL;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,2FAFK,EAGL;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,m2CAHK,EAKJ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,2sBALI,EAMR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,cAAK;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yEAAL,EACA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,4PADA,EAEA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kJAFA,EAGA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gLAHA,EAKA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,8XALA,CANQ,EAaR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,03FAbQ,EAgBR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,m1DAhBQ,EAmBR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,w7FAnBQ,EAsBR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,0fAtBQ,EAwBN;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAG,oBAAC,IAAD;AAAM,QAAA,EAAE,EAAC,GAAT;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kGAAH,CAxBM,CADJ;AA6BD;;;;EAhC4BF,S;;AAmC/B,eAAeE,gBAAf","sourcesContent":["import React, { Component } from 'react'\nimport { Link } from 'react-router-dom' \n\nclass ServiceFamilyLaw extends Component {\n\t\n  render() {\n    return (\n        <div className=\"item-container\">\n\t\t\t<div></div>\n\t\t\t<h2>Семейные споры</h2>\n\t\t\t<p>\n\t\t\t\tСемейная жизнь не всегда является источником стопроцентного позитива и радости. Иногда сложности в отношениях между супругами и ближайшими родственниками доходят до серьезных взаимных обвинений — таких, которые сложно разрешить без судебного вмешательства. </p>\n\t\t\t\t<p>Наш профессиональный юрист сможет избавить вас от этой головной боли и найти оптимальное решение, чтобы разрешить любые противоречия.</p>\n<ul> <li>Опекунство.</li>\n<li>Подготовка и расторжение брачных контрактов. </li>\n<li>Бракоразводные процессы.</li>\n<li>Присуждение выплаты алиментов</li>\n\n<li>И другие семейные проблемы поможет решить наш профессиональный юрист. </li></ul>\n\n<p>Обойтись без эксперта вряд ли получится, ведь отстаивание своих прав — довольно сложный процесс. Специалист не только будет представлять ваши интересы в суде, но и сможет решить проблемы. Разобраться с ситуацией для каждого клиента в индивидуальном порядке. У нас нет готовых шаблонов, по которым приходится действовать. Специалисты внимательно изучат ваше дело и подготовят все необходимые аргументы, составят план действий и обеспечат правомерную поддержку в суде. Отметим, что отсутствие адвоката по семейным делам — это заранее проигранный судебный процесс.</p>\n\n\n<p>Опытный адвокат может помочь правильно инициировать судебный процесс и разделить совместное имущество, разобраться в опеке над детьми. Для этого нужно знать определенные юридические тонкости и нюансы, которые помогут решить даже самое сложное дело. Наши профессиональные юристы действуют не эмоционально, а рационально и всегда принимают верные решения.</p>\n\n\n<p>Если вашей семейной жизни ничего не угрожает, не помешает быть предусмотрительными. Услуга по заключению брачного контракта сегодня является одной из самых востребованных. Вы можете обратиться к нашим юристам, чтобы оформить все надлежащим образом и обойтись без отрицательных последствий в случае необходимости. Ни одна семья не застрахована от проблем, разрешить которые не получится без профессионального и опытного юриста. Адвокат по семейным делам поможет разобраться и с жилищными проблемами, которые могут возникнуть из-за начавшегося бракоразводного процесса. </p>\n\n\n<p>Решите все семейные  вопросы без эмоций на высоком профессиональном уровне с нашей поддержкой!</p>\n\t\t\n\t\t<p><Link to='.'>Возврат к списку</Link></p>\t\n        </div>\n\n    );\n  }\n}\n\nexport default ServiceFamilyLaw;\n"]},"metadata":{},"sourceType":"module"}